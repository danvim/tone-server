    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "filereader": "^0.10.3",
    "peer": "^0.2.10",
    "peerjs-nodejs": "^1.1.3",
    "tone-core": "git+ssh://git@github.com/danvim/tone-core.git#master",
    "tslint": "^5.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
